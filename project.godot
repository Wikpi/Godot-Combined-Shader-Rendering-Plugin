; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "CompositeShadingManager",
"language": "GDScript",
"path": "res://addons/composite-shading/CompositeShadingManager.gd"
}, {
"base": "Node2D",
"class": "CompositeShadingTools",
"language": "GDScript",
"path": "res://addons/composite-shading/PluginTools.gd"
} ]
_global_script_class_icons={
"CompositeShadingManager": "",
"CompositeShadingTools": ""
}

[application]

config/name="CompositeShading"
run/main_scene="res://demo/DemoScene.tscn"
config/icon="res://demo/data/icon.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/composite-shading/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://demo/default_env.tres"
